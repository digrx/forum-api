{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "linebreak-style": "off",
      "no-underscore-dangle": "off",
      "object-curly-newline": "off",
      "class-methods-use-this": "off"
    }
}
